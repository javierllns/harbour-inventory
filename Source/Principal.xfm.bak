   WITH OBJECT ::oMariaDB := TMariaDBDataSource():New( Self )
      :cHost := "localhost"
      :cUser := "root"
      :cPassword := "1234"
      :cDatabase := "harbour-inventory"
      :lConnected := .T.
      :Create()
   END

   WITH OBJECT ::oGetAllProducts := TSQLQuery():New( Self )
      :oDataSource := ::oMariaDB
      :cProcess := "GENERAL"
      :nMaxRecords := 20
      :cSelect := "SELECT sku, name, description, stock_quantity, upc, price, usd_price, provider_id FROM products;"
      :Create()
   END

