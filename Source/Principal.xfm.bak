METHOD CreateForm( lCreate ) CLASS TPrincipal

   IF lCreate
      ::SetBounds( 469, 149, 800, 600 )
      ::SetClientSize( 784, 561 )
      ::cText := "Principal"
      ::nClrPane := clMenu
      ::nShowMode := smMAXIMIZE
      ::nMinWidth := 800
      ::nMinHeight := 600
      ::Create()
   ENDIF
   ::Super:CreateForm( .F. )

   WITH OBJECT ::cTitle := TBanner():New( Self )
      :SetBounds( 0, 0, 734, 50 )
      :cText := "SEA-INVENTORY"
      :nAlign := alTOP
      :nAlignMarginRight := 50
      :lParentFont := .F.
      :oFont := TFont():Create( "Century Gothic", 16, 0, 400 )
      :nClrText := clWhite
      :nClrPane := 0x8D732C
      :nClrPaneEnd := 0x8D732C
      :Create()
   END

   WITH OBJECT ::oNavPanel := TShiftPanel():New( Self )
      :SetBounds( 0, 50, 230, 511 )
      :cText := "Navigation"
      :lParentFont := .F.
      :oFont := TFont():Create( "Tahoma", 11, 0, 700 )
      :nClrPane := clWhite
      :nBorderStyle := bvHALFSUNKEN
      :nClrPaneEnd := clWhite
      :lAutoScroll := .F.
      :nBorderSides := bsRIGHT
      :nClrTitleText := 0x8D732C
      :nClrTitleStart := clWhite
      :nClrTitleEnd := clWhite
      :Create()
   END

      WITH OBJECT ::oNavPanelOverlay := TImage():New( ::oNavPanel )
         :SetBounds( 0, 258, 230, 230 )
         :nAnchors := akLEFTBOTTOM
         :nBorderStyle := bvNONE
         :oPicture := "sea-boxes.png"
         :lAutoFit := .T.
         :Create()
      END

      WITH OBJECT ::oButtonMod2 := TButtonMod():New( ::oNavPanel )
         :SetBounds( 8, 12, 214, 35 )
         :cText := "  ButtonMod1"
         :lParentFont := .F.
         :oFont := TFont():Create( "Tahoma", 11, 0, 400 )
         :nClrText := 0x414141
         :nClrPane := clWindow
         :nBorderRadius := 3
         :nClrBorder := 0xDFCB95
         :nClrTextDisabled := clSilver
         :nAlignment := taLEFT
         :Create()
      END

      WITH OBJECT ::oButtonMod3 := TButtonMod():New( ::oNavPanel )
         :SetBounds( 8, 12, 214, 32 )
         :cText := "  ButtonMod1"
         :lParentFont := .F.
         :oFont := TFont():Create( "Tahoma", 11, 0, 400 )
         :nClrPane := clMenu
         :nBorderRadius := 3
         :nAlignment := taLEFT
         :Create()
      END

      WITH OBJECT ::oButtonMod4 := TButtonMod():New( ::oNavPanel )
         :SetBounds( 8, 12, 214, 32 )
         :cText := "  ButtonMod1"
         :lParentFont := .F.
         :oFont := TFont():Create( "Tahoma", 11, 0, 400 )
         :nClrPane := clMenu
         :nBorderRadius := 3
         :nAlignment := taLEFT
         :Create()
      END

   WITH OBJECT ::oTitleImage := TImage():New( Self )
      :SetBounds( 732, 0, 54, 50 )
      :nAnchors := akTOPRIGHT
      :oPicture := "sea-boxes-light.png"
      :nBorderSides := bsNONE
      :lAutoFit := .T.
      :Create()
   END

   WITH OBJECT ::oPages := TPages():New( Self )
      :SetBounds( 230, 50, 554, 511 )
      :nAlign := alCLIENT
      :nIndex := 2
      ::oProductsPage := TPage():Create( ::oPages )

      ::oPage2 := TPage():Create( ::oPages )

      :Create()
   END

RETURN Self
